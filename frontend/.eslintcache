creen.js",["71","72"],"import React, { useState, useEffect } from \"react\";\nimport { Form, Button } from \"react-bootstrap\";\nimport FormContainer from \"../components/FormContainer\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport {\n  createComplaint,\n  deleteComplaint,\n} from \"../actions/complaintsActions.js\";\nimport axios from \"axios\";\n\nconst FormScreen = ({ history }) => {\n  const [name, setName] = useState(\"\");\n  const [email, setEmail] = useState(\"\");\n  const [text, setText] = useState(\"\");\n  const [drySoil, setDrySoil] = useState(false);\n  const [yellowLeaves, setYellowLeaves] = useState(false);\n  const [holesInLeaves, setHolesInLeaves] = useState(false);\n  const [uploading, setUploading] = useState(false);\n  const [image, setImage] = useState(null);\n  const dispatch = useDispatch();\n\n  const complaintState = useSelector((state) => state.getComplaint);\n\n  const { complaint } = complaintState;\n\n  const cancelComplaintHandler = () => {\n    dispatch(deleteComplaint(complaint._id));\n  };\n\n  const uploadFileHandler = async (e) => {\n    const file = e.target.files[0];\n    const formData = new FormData();\n    formData.append(\"image\", file);\n    setUploading(true);\n    setImage(file);\n    const config = {\n      headers: {\n        \"Content-type\": \"multipart/form-data\",\n      },\n    };\n    try {\n      await axios.post(\"/api/upload\", formData, config);\n      setUploading(false);\n    } catch {\n      console.error(e);\n      setUploading(false);\n    }\n  };\n  const submitHandler = (e) => {\n    e.preventDefault();\n    dispatch(\n      createComplaint({\n        name,\n        email,\n        text,\n        drySoil,\n        yellowLeaves,\n        holesInLeaves,\n        image,\n      })\n    );\n  };\n\n  return (\n    <>\n      <FormContainer>\n        <h1>What's Up</h1>\n        <Form onSubmit={submitHandler}>\n          <Form.Group controlId=\"name\">\n            <Form.Label>Name</Form.Label>\n            <Form.Control\n              type=\"name\"\n              placeholder=\"name\"\n              value={name}\n              onChange={(e) => setName(e.target.value)}\n            ></Form.Control>\n          </Form.Group>\n          <Form.Group controlId=\"email\">\n            <Form.Label>Email</Form.Label>\n            <Form.Control\n              type=\"email\"\n              placeholder=\" Email Address\"\n              value={email}\n              onChange={(e) => setEmail(e.target.value)}\n            ></Form.Control>\n          </Form.Group>\n          <Form.Group controlId=\"formBasicCheckbox\">\n            <Form.Check\n              type=\"checkbox\"\n              label=\"Dry soil\"\n              value={drySoil}\n              onChange={(e) => setDrySoil(e.target.value)}\n            />\n          </Form.Group>\n          <Form.Group controlId=\"formBasicCheckbox\">\n            <Form.Check\n              type=\"checkbox\"\n              label=\"Holes in soil\"\n              value={yellowLeaves}\n              onChange={(e) => setHolesInLeaves(e.target.value)}\n            />\n          </Form.Group>\n          <Form.Group controlId=\"formBasicCheckbox\">\n            <Form.Check\n              type=\"checkbox\"\n              label=\"Yellow leaves\"\n              value={holesInLeaves}\n              onChange={(e) => setYellowLeaves(e.target.value)}\n            />\n          </Form.Group>\n          <Form.Group controlId=\"text\">\n            <Form.Label>Complaint</Form.Label>\n            <Form.Control\n              as=\"textarea\"\n              placeholder=\"What happened?\"\n              value={text}\n              onChange={(e) => setText(e.target.value)}\n            ></Form.Control>\n            <Form.Group controlId=\"image\">\n              <Form.Label>Image</Form.Label>\n              <Form.File\n                id=\"image-file\"\n                label=\"Choose file\"\n                custom\n                value={image}\n                onChange={uploadFileHandler}\n              ></Form.File>\n            </Form.Group>\n          </Form.Group>\n          <Button type=\"submit\" variant=\"primary\">\n            Submit\n          </Button>\n          <Button onClick={cancelComplaintHandler}>Cancel</Button>\n        </Form>\n      </FormContainer>\n    </>\n  );\n};\n\nexport default FormScreen;\n","/Users/abiolajohnson/Documents/projects/Ekondo website/frontend/src/components/Navigation.js",[],"/Users/abiolajohnson/Documents/projects/Ekondo website/frontend/src/reducers/complaintReducer.js",[],"/Users/abiolajohnson/Documents/projects/Ekondo website/frontend/src/actions/complaintsActions.js",[],"/Users/abiolajohnson/Documents/projects/Ekondo website/frontend/src/constants/complaintsConstants.js",[],"/Users/abiolajohnson/Documents/projects/Ekondo website/frontend/src/screens/InfoScreen.js",[],"/Users/abiolajohnson/Documents/projects/Ekondo website/frontend/src/reducers/userReducers.js",[],"/Users/abiolajohnson/Documents/projects/Ekondo website/frontend/src/constants/userLoginConstants.js",[],"/Users/abiolajohnson/Documents/projects/Ekondo website/frontend/src/actions/userActions.js",[],{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","severity":1,"message":"78","line":2,"column":42,"nodeType":"79","messageId":"80","endLine":2,"endColumn":48},{"ruleId":"77","severity":1,"message":"81","line":1,"column":27,"nodeType":"79","messageId":"80","endLine":1,"endColumn":36},{"ruleId":"77","severity":1,"message":"82","line":3,"column":23,"nodeType":"79","messageId":"80","endLine":3,"endColumn":34},{"ruleId":"77","severity":1,"message":"83","line":10,"column":9,"nodeType":"79","messageId":"80","endLine":10,"endColumn":17},{"ruleId":"77","severity":1,"message":"81","line":1,"column":27,"nodeType":"79","messageId":"80","endLine":1,"endColumn":36},{"ruleId":"77","severity":1,"message":"84","line":18,"column":10,"nodeType":"79","messageId":"80","endLine":18,"endCo